    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@vitejs/plugin-legacy": "^1.7.0",
    "@vitejs/plugin-react": "^1.0.7",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
